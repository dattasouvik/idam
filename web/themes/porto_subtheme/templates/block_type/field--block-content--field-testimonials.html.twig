{% for key, item in items %}
  <div class="owl-item">
    <div>
      <div class="col-md-12">
        <div class="testimonial testimonial-style-2 testimonial-with-quotes mb-none">
          <blockquote>
            <p>{{ item.content['#field_collection_item'].field_tes_blockquote.value }}</p>
          </blockquote>
          <div class="testimonial-author">
            <p>
              <strong>{{ item.content['#field_collection_item'].field_tes.value }}</strong>
              <span>{{ item.content['#field_collection_item'].field_tes_unit.value }}</span>
            </p>
          </div>
        </div>
      </div>
    </div>
  </div>
{% endfor %}

