{%
  set classes = [
    'block',
    'block-' ~ configuration.provider|clean_class,
    'block-' ~ plugin_id|clean_class,
  ]
%}
<div{{ attributes.addClass(classes) }}>
  {{ title_prefix }}
  {% block content %}
    <div class="container">
      <div class="row">
        <div class="col-md-12 center">
          {{content}}
        </div>
      </div>
    </div>
  {{ title_suffix }}
</div>
