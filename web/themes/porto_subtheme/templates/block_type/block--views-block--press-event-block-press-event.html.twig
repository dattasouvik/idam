{%
  set classes = [
    'block',
    'block-' ~ configuration.provider|clean_class,
    'block-' ~ plugin_id|clean_class,
  ]
%}
<div{{ attributes.addClass(classes) }}>
  {{ title_prefix }}
  <section class="section section-default mb-none">
    <div class="container pb-xlg">
      {{ content }}
    </div>
  </section>  
  {{ title_suffix }}
</div>
