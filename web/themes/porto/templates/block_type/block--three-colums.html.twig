{%
  set classes = [
    'block',
    'block-' ~ configuration.provider|clean_class,
    'block-' ~ plugin_id|clean_class,
  ]
%}
<div{{ attributes.addClass(classes) }}>
  {{ title_prefix }}
  <section class="section section-default mb-none">
    <div class="container">
      <div class="row">
        <div class="col-md-4">
          {{content.field_info_phone}}
        </div>
        <div class="col-md-4">
          {{content.field_info_time}}
        </div>
        <div class="col-md-4" id="send-message">
          <h5 class="mb-md mt-xl">Send a Message</h5>
          {{content.field_send_a_message}}
        </div>
      </div>
    </div>
  </section>
  {{ title_suffix }}
</div>